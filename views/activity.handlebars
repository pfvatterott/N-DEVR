<section class="section">
  </div>
  <div class="row container">
    <h2>Map Test</h2>
  </div>
  <div class="row container">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input placeholder="Activity Name" id="activity_name" type="text" class="validate">
          <label for="activity_name">Activity Name</label>
        </div>
      </div>
    </form>
  </div>
  <div class="row container">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <textarea id="textarea1" class="materialize-textarea"></textarea>
          <label for="textarea1">Textarea</label>
        </div>
      </div>
    </form>
  </div>
  <div class="row container">
    <div class="col s12 m4">
      <ul class="collection" id="activityList">
      </ul>
    </div>
    <div class="col s12 m8">
       <div id="mapid" style="height: 500px"></div>
    </div>
  </div>
  <div class="row container">
    <h4 id="totalDistance"></h4>
    <h4 id="totalElevation"></h4>
    <div id="chartContainer" style="height: 370px; width: 100%;"></div>
    <a class="waves-effect waves-light btn save-activity" name="save-activity" id="save-activity">Save Activity</a>

  </div>
</section>